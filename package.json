{
  "name": "MoverMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "yarn rnuc:dev && react-native run-android --appIdSuffix dev --main-activity MainActivity",
    "android:prod": "yarn rnuc:prod && react-native run-android",
    "android:stage": "yarn rnuc:stage && react-native run-android --appIdSuffix stage --main-activity MainActivity",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "patch-package",
    "android-release": "yarn rnuc:dev && cd ./android && ./gradlew assembleRelease",
    "android-release:prod": "yarn rnuc:prod && cd ./android && ./gradlew assembleRelease",
    "android-release:stage": "yarn rnuc:stage && cd ./android && ./gradlew assembleRelease",
    "rnuc:dev": "yarn rnuc ./app/envs/dev.yml",
    "rnuc:stage": "yarn rnuc ./app/envs/stage.yml",
    "rnuc:prod": "yarn rnuc ./app/envs/prod.yml"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-community/eslint-config": "^2.0.0",
    "@react-native-community/netinfo": "^9.3.6",
    "@walletconnect/react-native-dapp": "^1.8.0",
    "@walletconnect/web3-provider": "^1.8.0",
    "apisauce": "^2.1.6",
    "crypto-browserify": "^3.12.0",
    "dayjs": "^1.11.6",
    "ethers": "^5.7.2",
    "i18n-js": "4.1.1",
    "mobx": "^6.6.2",
    "mobx-react-lite": "^3.4.0",
    "mover-icon-set": "git+https://github.com/viaMover/mover-icon-set.git",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "react": "18.1.0",
    "react-ioc": "^1.0.0",
    "react-native": "0.70.4",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-randombytes": "^3.6.1",
    "react-native-reanimated": "^2.12.0",
    "react-native-svg": "^12.4.4",
    "react-native-ui-lib": "^6.22.3",
    "react-native-ultimate-config": "^4.0.0",
    "react-native-vector-icons": "^9.2.0",
    "stream-browserify": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.20.0",
    "@babel/runtime": "^7.12.5",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "node-libs-react-native": "^1.2.1",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "resolutions": {
    "react-native-svg": "^12.4.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
